{
    "name": "nova-kit/nova-devtool",
    "description": "Vue DevTool for Laravel Nova",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "NovaKit\\NovaDevTool\\": "src/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "orchestra/workbench": "^0.2.4"
    },
    "require-dev": {
        "laravel/nova": "^4.14",
        "nunomaduro/larastan": "^1.0.1 || ^2.0",
        "orchestra/testbench": "^6.35 || ^7.29 || ^8.9"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "NovaKit\\NovaDevTool\\LaravelServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi",
            "@php ./vendor/bin/testbench nova:publish"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
